# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                 	controllers.Application.index
GET     /productsArgs       controllers.Application.product(prodType, prodNumber: Int)
GET     /product/:prodType/:prodNumber       controllers.Application.product(prodType, prodNumber: Int)
GET     /productRE/$prodType<[a-z]{2}\d\d>/:prodNumber       controllers.Application.product(prodType, prodNumber: Int)

GET     /taskList1          controllers.TaskList1.taskList

# Prefix must match `play.assets.urlPrefix`
GET        /assets/*file                 controllers.Assets.at(file)
GET        /versionedAssets/*file        controllers.Assets.versioned(path="/public", file: Asset)
